  "homepage": "https://bitbucket.org/bytecode-lm/content-chef-delivery-js-client#readme",
  "devDependencies": {
    "chai": "3.4.1",
    "commitizen": "2.4.6",
    "cz-conventional-changelog": "1.1.4",
    "ghooks": "1.0.1",
    "mocha": "2.3.4"
  },
  "dependencies": {
    "axios": "0.7.0",
    "es6-promise": "3.0.2"
  },
  "czConfig": {
